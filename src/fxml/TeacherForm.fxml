<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER" prefHeight="167.0" prefWidth="251.0">
         <children>
            <Label text="Label">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <AnchorPane>
               <children>
                  <Label text="First Name">
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </Label>
                  <TextField fx:id="firstNameField" layoutX="158.0" layoutY="6.0" prefHeight="26.0" prefWidth="150.0" AnchorPane.rightAnchor="5.0">
                     <cursor>
                        <Cursor fx:constant="V_RESIZE" />
                     </cursor>
                  </TextField>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </AnchorPane>
            <AnchorPane prefHeight="26.0" prefWidth="149.0">
               <children>
                  <Label text="Last Name">
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </Label>
                  <TextField fx:id="lastNameField" layoutX="158.0" layoutY="6.0" prefHeight="26.0" prefWidth="150.0" AnchorPane.rightAnchor="5.0" />
               </children>
            </AnchorPane>
            <AnchorPane prefHeight="26.0" prefWidth="150.0">
               <children>
                  <Label text="Birthdate">
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </Label>
                  <DatePicker fx:id="birthDatePicker" layoutX="158.0" layoutY="6.0" prefHeight="26.0" prefWidth="150.0" AnchorPane.rightAnchor="5.0" />
               </children>
            </AnchorPane>
            <AnchorPane>
               <children>
                  <Label text="Email">
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </Label>
                  <TextField fx:id="emailField" layoutX="158.0" layoutY="6.0" prefHeight="26.0" prefWidth="150.0" AnchorPane.rightAnchor="5.0" />
               </children>
               <padding>
                  <Insets bottom="5.0" top="5.0" />
               </padding>
            </AnchorPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
